                                    <% }else if(typeof(user) !== 'undefined' && !user.isAuthenticated){ %>
                                    <a href="/verify-email" class="btn">Vertify your email!</a>
                                    <% } %>
                                 </div>
                              </div>
                           </div>
                        </div>
                     </div>
                  </div>
                  </div>
                  <% }); %>
               </div>
