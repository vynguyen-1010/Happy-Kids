                                    data-y="center"
                                    data-hoffset="0"
                                    data-voffset="0"
                                    data-speed="600"
                                    data-start="900"
                                    data-easing="Power4.easeOut"
                                    data-endeasing="Power4.easeIn">
                                    <div class="tp-caption1-wd-1 tt-base-color"><%= content.title1 %></div>
                                    <div class="tp-caption1-wd-2"><%= content.title2 %> </div>
                                    <div class="tp-caption1-wd-3"><%= content.title3 %></div>
                                    <div class="tp-caption1-wd-4"><a href="/products" target="_blank" class="btn btn-xl" data-text="Shop Now!">Shop Now!</a></div>
                                 </div>
